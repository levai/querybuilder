{
  "name": "vue-querybuilder",
  "version": "1.0.0-beta.1",
  "description": "Vue 3 Query Builder component - 1:1 port of react-querybuilder using @react-querybuilder/core",
  "type": "module",
  "main": "./dist/vue-querybuilder.cjs",
  "module": "./dist/vue-querybuilder.js",
  "types": "./dist/vue-querybuilder.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/vue-querybuilder.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/vue-querybuilder.cjs"
      }
    },
    "./formatQuery": {
      "import": {
        "types": "./dist/fwd/formatQuery.d.ts",
        "default": "./dist/fwd/formatQuery.js"
      },
      "require": {
        "types": "./dist/fwd/formatQuery.d.ts",
        "default": "./dist/fwd/formatQuery.cjs"
      }
    },
    "./parseCEL": {
      "import": {
        "types": "./dist/fwd/parseCEL.d.ts",
        "default": "./dist/fwd/parseCEL.js"
      },
      "require": {
        "types": "./dist/fwd/parseCEL.d.ts",
        "default": "./dist/fwd/parseCEL.cjs"
      }
    },
    "./parseJSONata": {
      "import": {
        "types": "./dist/fwd/parseJSONata.d.ts",
        "default": "./dist/fwd/parseJSONata.js"
      },
      "require": {
        "types": "./dist/fwd/parseJSONata.d.ts",
        "default": "./dist/fwd/parseJSONata.cjs"
      }
    },
    "./parseJsonLogic": {
      "import": {
        "types": "./dist/fwd/parseJsonLogic.d.ts",
        "default": "./dist/fwd/parseJsonLogic.js"
      },
      "require": {
        "types": "./dist/fwd/parseJsonLogic.d.ts",
        "default": "./dist/fwd/parseJsonLogic.cjs"
      }
    },
    "./parseMongoDB": {
      "import": {
        "types": "./dist/fwd/parseMongoDB.d.ts",
        "default": "./dist/fwd/parseMongoDB.js"
      },
      "require": {
        "types": "./dist/fwd/parseMongoDB.d.ts",
        "default": "./dist/fwd/parseMongoDB.cjs"
      }
    },
    "./parseSpEL": {
      "import": {
        "types": "./dist/fwd/parseSpEL.d.ts",
        "default": "./dist/fwd/parseSpEL.js"
      },
      "require": {
        "types": "./dist/fwd/parseSpEL.d.ts",
        "default": "./dist/fwd/parseSpEL.cjs"
      }
    },
    "./parseSQL": {
      "import": {
        "types": "./dist/fwd/parseSQL.d.ts",
        "default": "./dist/fwd/parseSQL.js"
      },
      "require": {
        "types": "./dist/fwd/parseSQL.d.ts",
        "default": "./dist/fwd/parseSQL.cjs"
      }
    },
    "./transformQuery": {
      "import": {
        "types": "./dist/fwd/transformQuery.d.ts",
        "default": "./dist/fwd/transformQuery.js"
      },
      "require": {
        "types": "./dist/fwd/transformQuery.d.ts",
        "default": "./dist/fwd/transformQuery.cjs"
      }
    },
    "./dist/*": "./dist/*"
  },
  "files": ["dist", "dist/styles", "dist/fwd"],
  "sideEffects": ["*.css", "*.scss"],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-querybuilder/react-querybuilder.git",
    "directory": "packages/vue-querybuilder"
  },
  "keywords": ["vue", "vue3", "querybuilder", "query", "builder", "operators", "component", "composition-api"],
  "license": "MIT",
  "homepage": "https://react-querybuilder.js.org/",
  "scripts": {
    "start": "vite",
    "build": "bun --bun tsdown && bun run build:css && vue-tsc -p tsconfig.build.json",
    "build:main": "bun --bun tsdown",
    "build:css": "mkdir -p dist/styles && cp ../core/src/*.scss dist && cp ../core/src/styles/*.scss dist/styles && bun sass --style=compressed --silence-deprecation=if-function dist",
    "typecheck": "vue-tsc --noEmit",
    "test": "bunx vitest run"
  },
  "dependencies": {
    "@react-querybuilder/core": "workspace:*",
    "@vue-dnd-kit/core": "^1.7.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^25.0.0",
    "sass": "catalog:",
    "typescript": "catalog:",
    "unplugin-vue": "^7.1.1",
    "vite": "catalog:",
    "vitest": "^2.1.6",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  }
}
